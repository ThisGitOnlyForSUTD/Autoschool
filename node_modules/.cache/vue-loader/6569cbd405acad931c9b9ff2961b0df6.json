<v-list-item\n          v-for=\"([icon, text, link], i) in items\"\n          :key=\"i\"\n          link\n          @click=\"$vuetify.goTo(link)\"\n        >\n          <v-list-item-content>\n            <v-list-item-title class=\"subtitile-1\">{{\n              text\n            }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar\n      app\n      :color=\"color\"\n      :flat=\"flat\"\n      dark\n      class=\"px-15\"\n      :class=\"{ expand: flat }\"\n    >\n      <v-toolbar-title>\n        <v-img src=\"@/assets/img/logo.png\" max-width=\"50px\" />\n      </v-toolbar-title>\n      <v-spacer />\n      <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        class=\"mr-4\"\n        v-if=\"isXs\"\n      />\n      <div v-else>\n        <v-btn text @click=\"$vuetify.goTo('#download')\">\n          <span class=\"mr-2\">О нас</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#features')\">\n          <span class=\"mr-2\">Документы</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#carusel')\">\n          <span class=\"mr-2\">Галерея</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#contact')\">\n          <span class=\"mr-2\">Контакты</span>\n        </v-btn>\n        <v-btn class=\"translateLang\" text @click=\"ChangeLang\">\n          rus\n        </v-btn>\n      </div>\n    </v-app-bar>\n  </div>\n</template>\n\n<style scoped>\n.v-toolbar {\n  transition: 0.6s;\n}\n\n.expand {\n  height: 80px !important;\n  padding-top: 10px;\n}\n</style>\n\n<script>\nexport default {\n  data: () => ({\n    drawer: null,\n    isXs: false,\n    items: [\n      [\"mdi-home-outline\", \"O нас\", \"#hero\"],\n      [\"mdi-information-outline\", \"Документы\", \"#features\"],\n      [\"mdi-download-box-outline\", \"Галерея\", \"#download\"],\n      [\"mdi-email-outline\", \"Контакты\", \"#contact\"],\n    ],\n  }),\n  props: {\n    color: String,\n    flat: Boolean,\n  },\n  methods: {\n    onResize() {\n      this.isXs = window.innerWidth < 850;\n    },\n  },\n\n  watch: {\n    isXs(value) {\n      if (!value) {\n        if (this.drawer) {\n          this.drawer = false;\n        }\n      }\n    },\n  },\n  mounted() {\n    this.onResize();\n    window.addEventListener(\"resize\", this.onResize, { passive: true });\n  },\n};\n</script>\n"]}]}